{
  "author": "Rich W <richard@smg.digital>",
  "name": "x-tunnel-server",
  "description": "port forward without the port forwarding",
  "version": "1.3.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/smgdesign/x-tunnel.git"
  },
  "dependencies": {
    "async": "3.2.3",
    "book": "1.3.3",
    "debug": "3.1.0",
    "dotenv": "16.0.1",
    "esm": "3.0.34",
    "human-readable-ids": "1.0.3",
    "koa": "2.5.1",
    "koa-router": "7.4.0",
    "localenv": "0.2.2",
    "optimist": "0.6.1",
    "pump": "3.0.0",
    "tldjs": "2.3.1",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "@types/async": "3.2.13",
    "@types/concurrently": "7.0.0",
    "@types/debug": "4.1.7",
    "@types/eslint": "8.4.2",
    "@types/koa": "2.13.4",
    "@types/koa-router": "7.4.4",
    "@types/node": "17.0.38",
    "@types/optimist": "0.0.30",
    "@types/pump": "1.1.1",
    "@types/request": "2.48.8",
    "@types/request-promise": "4.1.48",
    "@types/shelljs": "0.8.11",
    "@types/tldjs": "2.3.1",
    "@types/winston": "2.4.4",
    "concurrently": "7.2.1",
    "eslint": "8.16.0",
    "mocha": "5.1.1",
    "node-dev": "3.1.3",
    "nodemon": "2.0.16",
    "shelljs": "0.8.5",
    "supertest": "3.1.0",
    "ts-node": "10.8.0",
    "typescript": "4.7.2",
    "ws": "5.1.1"
  },
  "scripts": {
    "test": "mocha --check-leaks --require esm './**/*.test.js'",
    "build": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "nodemon --inspect dist/server.js --port 3001",
    "serve": "node dist/server.js",
    "start": "npm run serve -- --port $npm_config_port",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js --port 3001 --domain local.x-tunnel.x-smg.com",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
